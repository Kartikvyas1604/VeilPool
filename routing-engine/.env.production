# Production Environment Configuration
NODE_ENV=production
PORT=3001

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=

# Solana Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_WS_URL=wss://api.mainnet-beta.solana.com

# Pyth Network Oracle
PYTH_PRICE_FEED_ID=YOUR_MAINNET_PRICE_FEED_ID
PYTH_PROGRAM_ID=YOUR_PYTH_PROGRAM_ID

# Monitoring & Observability
LOG_LEVEL=info
ENABLE_METRICS=true
METRICS_PORT=9090

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Node Health Check
HEALTH_CHECK_INTERVAL=60000
HEALTH_CHECK_TIMEOUT=5000

# Cache TTL (seconds)
CACHE_TTL_ROUTING=300
CACHE_TTL_NODE_RANKING=300
CACHE_TTL_THREAT_INTEL=30

# API Security
API_KEY=
CORS_ORIGIN=https://veilpool.app
